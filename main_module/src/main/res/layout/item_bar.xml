<?xml version="1.0" encoding="utf-8"?>
<layout>
    <data>
        <variable
            name="entity"
            type="com.rwz.lib_comm.entity.extension.SimpleEntity" />
        <variable
            name="viewModule"
            type="com.rwz.lib_comm.base.BaseViewModule" />
    </data>

    <RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <TextView style="@style/text_dark"
            android:textSize="@dimen/text_17"
            android:padding="@dimen/h_10"
            android:text="@{entity.data.toString()}"
            />

        <View style="@style/line"
            android:layout_alignParentBottom="true"
            android:layout_marginTop="@dimen/v_12"
            />

    </RelativeLayout>
</layout>